{
  "name": "misantron/sendgrid-api",
  "description": "SendGrid API PHP client",
  "keywords": [
    "sendgrid",
    "api",
    "php"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.4 | ^8.0",
    "guzzlehttp/guzzle": "^7.0",
    "symfony/options-resolver": "^5.4 | ^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "Misantron\\SendGrid\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Misantron\\SendGrid\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  }
}
